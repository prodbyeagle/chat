{
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"bracketSameLine": true,
	"importOrder": [
			"<THIRD_PARTY_MODULES>",
			"",
			"^@/app/(.*)$", 
			"",
			"^@/lib/(.*)$", 
			"",
			"^@/providers/(.*)$",
			"",
			"^@/types/(.*)$", 
			"",
			"^@/utils/(.*)$",
			"",
			"^[./]"
		],
	"plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
